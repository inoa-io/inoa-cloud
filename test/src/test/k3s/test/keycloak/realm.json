{
	"enabled": true,
	"realm": "inoa",
	"displayName": "INOA",
	"loginTheme": null,
	"emailTheme": null,
	"internationalizationEnabled": false,
	"defaultLocale": "en",
	"supportedLocales": [ "de", "en", "it" ],
	"duplicateEmailsAllowed": false,
	"loginWithEmailAllowed": true,
	"verifyEmail": true,
	"accessTokenLifespan": 86400,
	"accessTokenLifespanForImplicitFlow": 86400,
	"ssoSessionIdleTimeout": 86400,
	"ssoSessionMaxLifespan": 86400,
	"attributes": {
		"frontendUrl": "http://keycloak.${k3s.ip}.nip.io:8080"
	},
  "browserSecurityHeaders": {
    "contentSecurityPolicyReportOnly": "",
    "xContentTypeOptions": "nosniff",
    "referrerPolicy": "no-referrer",
    "xRobotsTag": "none",
    "xFrameOptions": "",
    "contentSecurityPolicy": "default-src http: 'unsafe-eval' 'unsafe-inline'; object-src 'none'",
    "xXSSProtection": "1; mode=block",
    "strictTransportSecurity": "max-age=31536000; includeSubDomains"
  },
	"groups": [
		{
			"id": "inoa",
			"name": "inoa",
			"path": "/inoa",
			"attributes": {},
			"realmRoles": [],
			"clientRoles": {},
			"subGroups": []
		}
	],
	"users": [
		{
			"id": "inoa-admin",
			"enabled": true,
			"username": "admin",
			"firstName": "admin",
			"lastName": "admin",
			"email": "admin@example.org",
			"emailVerified": true,
			"credentials": [ { "type": "password", "value": "password" } ],
			"clientRoles": { "realm-management": [ "realm-admin" ] },
			"groups": [ "inoa" ]
		}
	],
	"clients": [
		{
			"id": "inoa-grafana",
			"enabled": true,
			"clientId": "grafana",
			"publicClient": false,
			"secret": "changeMe",
			"standardFlowEnabled": true,
			"implicitFlowEnabled": false,
			"directAccessGrantsEnabled": false,
			"serviceAccountsEnabled": false,
			"protocol": "openid-connect",
			"protocolMappers": [],
			"baseUrl": "http://grafana.${k3s.ip}.nip.io:8080",
			"redirectUris": [ "http://grafana.${k3s.ip}.nip.io:8080/*", "http://grafana.127.0.0.1.nip.io:8080/*"],
			"webOrigins": [ "http://grafana.${k3s.ip}.nip.io:8080/*", "http://grafana.127.0.0.1.nip.io:8080/*"],
			"defaultClientScopes": [ "email" ],
			"optionalClientScopes": []
		},
    {
      "id": "inoa-groundcontrol",
      "enabled": true,
      "clientId": "inoa-groundcontrol",
      "secret": "changeMe",
      "publicClient": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "attributes": {
        "client_credentials.use_refresh_token": "false",
        "post.logout.redirect.uris": "http://inoa.${k3s.ip}.nip.io:8080/logout"
      },
      "protocol": "openid-connect",
      "protocolMappers": [
        {
          "name": "audience",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "config": {
            "included.client.audience": "elco-burner-selector-app",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        },{
          "name": "realm-roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "config": {
            "claim.name": "roles",
            "multivalued": "true",
            "id.token.claim": "true",
            "jsonType.label": "String",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        }
      ],
      "baseUrl": "http://inoa.${k3s.ip}.nip.io:8080",
      "redirectUris": [
        "http://localhost:4200/*",
        "http://localhost:4300/*",
        "http://swagger.${k3s.ip}.nip.io:8080/*",
        "http://inoa.${k3s.ip}.nip.io:8080/oauth/callback/inoa-groundcontrol"
      ],
      "webOrigins": [ "*" ],
      "defaultClientScopes": [ "profile", "email" ],
      "optionalClientScopes": []
    },{
      "id": "inoa-cloud",
      "enabled": true,
      "clientId": "inoa-cloud",
      "secret": "changeMe",
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "protocol": "openid-connect",
      "protocolMappers": [
        {
          "name": "role-view-realm",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-hardcoded-role-mapper",
          "config": {
            "role": "realm-management.view-realm"
          }
        },
        {
          "name": "role-view-users",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-hardcoded-role-mapper",
          "config": {
            "role": "realm-management.view-users"
          }
        },
        {
          "name": "role-manage-users",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-hardcoded-role-mapper",
          "config": {
            "role": "realm-management.manage-users"
          }
        },{
          "name": "claim",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-client-role-mapper",
          "config": {
            "usermodel.clientRoleMapping.clientId":"realm-management",
            "multivalued": "true",
            "claim.name": "resource_access.${client_id}.roles",
            "userinfo.token.claim": "false",
            "id.token.claim": "false",
            "access.token.claim": "true"
          }
        }
      ],
      "attributes": {
        "post.logout.redirect.uris": "http://inoa.${inoa.k3s}.nip.io:8080/logout",
        "access.token.lifespan": "60"
      },
      "redirectUris": [],
      "webOrigins": [],
      "defaultClientScopes": [],
      "optionalClientScopes": []
		},{
			"id": "inoa-mcnoize",
			"enabled": true,
			"clientId": "inoa-mcnoize",
			"publicClient": false,
			"secret": "changeMe",
			"standardFlowEnabled": false,
			"implicitFlowEnabled": false,
			"directAccessGrantsEnabled": true,
			"serviceAccountsEnabled": true,
			"protocol": "openid-connect",
			"protocolMappers": [
				{
					"name": "audience",
					"protocol": "openid-connect",
					"protocolMapper": "oidc-audience-mapper",
					"config": {
						"included.custom.audience": "inoa-registry",
						"id.token.claim": "true",
						"access.token.claim": "true"
					}
				},{
					"name": "tenants",
					"protocol": "openid-connect",
					"protocolMapper": "oidc-hardcoded-claim-mapper",
					"config": {
						"access.token.claim": "true",
						"claim.name": "tenants",
						"claim.value": "inoa"
					}
				}
			],
			"baseUrl": null,
			"redirectUris": [],
			"webOrigins": [],
			"defaultClientScopes": [],
			"optionalClientScopes": []
		},{
			"id": "inoa-swagger",
			"enabled": true,
			"clientId": "inoa-swagger",
			"standardFlowEnabled": false,
			"implicitFlowEnabled": true,
			"directAccessGrantsEnabled": true,
			"serviceAccountsEnabled": false,
			"publicClient": true,
			"protocol": "openid-connect",
			"protocolMappers": [
				{
					"name": "audience",
					"protocol": "openid-connect",
					"protocolMapper": "oidc-audience-mapper",
					"config": {
						"included.custom.audience": "inoa-registry",
						"id.token.claim": "false",
						"access.token.claim": "true"
					}
				},
				{
					"name": "Group-to-Tenant Mapper",
					"protocol": "openid-connect",
					"protocolMapper": "oidc-group-membership-mapper",
					"consentRequired": false,
					"config": {
						"full.path": "false",
						"id.token.claim": "true",
						"access.token.claim": "true",
						"claim.name": "tenants",
						"userinfo.token.claim": "false"
					}
				},
				{
					"name": "realm-roles",
					"protocol": "openid-connect",
					"protocolMapper": "oidc-usermodel-realm-role-mapper",
					"config": {
						"claim.name": "roles",
						"multivalued": "true",
						"id.token.claim": "true",
						"jsonType.label": "String",
						"access.token.claim": "true",
						"userinfo.token.claim": "true"
					}
				}
			],
			"baseUrl": "http://swagger.${k3s.ip}.nip.io:8080/",
			"redirectUris": [
				"http://swagger.${k3s.ip}.nip.io:8080/oauth2-redirect.html"
			],
			"webOrigins": [ "*" ],
			"defaultClientScopes": [ "profile", "email" ],
			"optionalClientScopes": []
		},{
			"id": "hawkbit",
			"enabled": true,
			"clientId": "hawkbit",
			"standardFlowEnabled": false,
			"implicitFlowEnabled": true,
			"directAccessGrantsEnabled": true,
			"serviceAccountsEnabled": false,
			"publicClient": true,
			"protocol": "openid-connect",
			"protocolMappers": [
				{
					"name": "Group-to-Tenant Mapper",
					"protocol": "openid-connect",
					"protocolMapper": "oidc-group-membership-mapper",
					"consentRequired": false,
					"config": {
						"full.path": "false",
						"id.token.claim": "true",
						"access.token.claim": "true",
						"claim.name": "tenants",
						"userinfo.token.claim": "false"
					}
				},
				{
					"name": "realm-roles",
					"protocol": "openid-connect",
					"protocolMapper": "oidc-usermodel-realm-role-mapper",
					"config": {
						"claim.name": "roles",
						"multivalued": "true",
						"id.token.claim": "true",
						"jsonType.label": "String",
						"access.token.claim": "true",
						"userinfo.token.claim": "true"
					}
				}
			],
			"baseUrl": "http://hawkbit.${k3s.ip}.nip.io:8080/",
			"redirectUris": [
				"http://hawkbit.${k3s.ip}.nip.io:8080/oauth2-redirect.html"
			],
			"webOrigins": [ "*" ],
			"defaultClientScopes": [ "profile", "email" ],
			"optionalClientScopes": []
		}
	],
	"eventsEnabled": true,
	"eventsListeners": [ "metrics-listener", "jboss-logging" ],
	"adminEventsEnabled": true,
	"adminEventsDetailsEnabled": true
}
